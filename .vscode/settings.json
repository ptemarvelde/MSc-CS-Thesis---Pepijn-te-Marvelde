{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.drawio$",
                "cmd": "echo 'converting ${file}.'; drawio --export -r -f pdf --crop ${file}"
            }
        ]
    },
    "editor.formatOnSave": true,
    "latex-workshop.latex.outDir": "%DIR%/aux",
    "cSpell.words": [
        "DAIC",
        "xlarge"
    ],
}
