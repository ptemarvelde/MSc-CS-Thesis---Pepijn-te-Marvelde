{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.drawio$",
                "cmd": "echo 'converting ${file}'; filepath=$(echo '${file}' | sed 's|c:|/c|g' | sed 's|\\\\\\\\|/|g'); drawio --export -r -f pdf --crop $filepath;"
            }
        ]
    },
    "editor.formatOnSave": true,
    "latex-workshop.latex.outDir": "%DIR%/aux",
    "cSpell.words": [
        "Amalur",
        "DAIC",
        "Generalizability",
        "runtimes",
        "Santoku",
        "SOTA",
        "xlarge"
    ],
    "cSpell.ignoreWords": [
        "midrule"
    ],
}